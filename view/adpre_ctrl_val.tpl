<div class='variables'>
  <div class='variables_input'>
    <label for="{{name}}">{{name}}</label>
    <input id="{{name}}" type ="range" min ="{{min}}" max="{{max}}" step ="{{step}}" value ="{{value}}"/>
    <input id="{{name}}_val" type="text" value ="{{value}}">
  </div>
  <div id="chart_{{name}}" class='chart'></div>
</div>
